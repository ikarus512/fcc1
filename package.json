    "stop": "node server/stop.js",
    "lint": "jshint .",
    "test": "npm run lint && npm run test-int && npm run test-e2e",
    "test-e2e": "SET NODE_ENV=test-e2e&&    protractor test/e2e/protractor-conf.js",
    "pretest-e2e": "SET NODE_ENV=test-e2e&&    npm run stop && start /MIN node server/index.js && node server/wait.js",
    "posttest-e2e": "npm run stop",
    "test-int": "SET NODE_ENV=test-int&&    mocha test/integr/**/*.js --reporter dot --timeout 15000",
    "test-int-cov": "SET NODE_ENV=test-int-cov&&    istanbul cover ./node_modules/mocha/bin/_mocha -- test/integr/**/*.js --reporter dot --timeout 15000",
    "test-uni": "karma start ./test/unit/karma.conf.js --single-run"
  },
